# This file contains example environment variables for the Agent12 platform
# Copy this to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Twilio Configuration
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token

# Grok AI Configuration
GROK_API_KEY=your-grok-api-key
GROK_VOICE_API_URL=wss://api.x.ai/v1/voice

# Server Configuration
WEBSOCKET_SERVER_PORT=8080
BASE_URL=https://your-domain.com

# Stripe Configuration (for wallet payments)
# Get these from: https://dashboard.stripe.com/test/apikeys
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
# Webhook secret from: https://dashboard.stripe.com/test/webhooks
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret

# Frontend Environment Variables (Next.js)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
